{
  "compilerOptions": {
    "experimentalDecorators": true,
    "jsx": "react",
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "src",
    "paths": {
      "app/*": ["app/*"],
      "store/*": ["store/*"],
      "services/*": ["services/*"],
      "utils/*": ["utils/*"],
      "styles/*": ["styles/*"],
      "assets/*": ["assets/*"],
      "libs/*": ["libs/*"]
    }
  }
}
